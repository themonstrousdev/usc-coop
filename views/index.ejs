    <div class="inner-banner-large yit-cover-bg bg-overlay" style="background-image:url(images/bg-items/default.jpg);" data-stellar-background-ratio="0.5">
      <div class="container">
        <div class="row">
          <div class="col">
            <div class="text-center white-color-text">
              <div class="d-flex align-items-center height-xxsmall-screen justify-content-center">
                <div class="inner-banner-content">
                  <h3>University of San Carlos - Cooperative</h3>
                  <p class="yit-lead"> A Cooperative made by the University of San Carlos </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
